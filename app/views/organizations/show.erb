<div class="col-lg-12">


  <div class="col-lg-6">
    <img src="https://upload.wikimedia.org/wikipedia/commons/e/e5/Seine_wide.jpg" alt="..." class="img-thumbnail">
    <h1 class = "profile-name"> <%= current_org.name%> </h1>
    <a href = '/organizations/posts'> <div class = "btn-primary"> Add a post </div> </a>
    <div class = "profile-box">
      <div class = "format">
        <h3> Organization Info </h3>
        <h4> Email: </h4> <%= current_org.email%>
        <h4> Phone Number: </h4> <%= current_org.phone_number%>
      </div>
    </div>
  </div>


  <div class = "col-lg-6">
    <div class = "profile-box">
      <div class = "format">
        <div class = "vertical-space-format">
          <h3> Interested Students</h3>
          <%= erb :'partials/_students_view' %>
        </div>
      </div>
    </div>
  </div>

</div>