<h2>Register</h2>
<%= erb :'partials/_errors' %>
<form method="post" action="/organizations">
  <div class="form-group">
     <label>Organization Name</label>
    <input type="text" name="org[name]" class="form-control" placeholder="Organization Name">
  </div>
  <div class="form-group">
     <label>Email address</label>
    <input type="email" name="org[email]" class="form-control" placeholder="Email">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input type="password" name="password" class="form-control" placeholder="Password">
    <input type="password" name="password2" class="form-control" placeholder="Enter Password Again">
  </div>
  <button type="submit" class="btn btn-default">Register</button>
</form>